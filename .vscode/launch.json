    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "CGO_ENABLED": "0"
            },
            "args": []
        },
        {
            "name": "Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${applicationFolder}/server.go",
            "env": {
                "CGO_ENABLED": "0"
            },
            "args": [
                "run",
                "--verbose=true",
                "--debug-webui-proxy=http://localhost:3000",
                "testdata/in-bhojpur-config.yaml"
            ]
        },
        {
            "name": "Debug Job Log",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${applicationFolder}/client.go",
            "env": {
                "CGO_ENABLED": "0"
            },
            "buildFlags": "-tags client",
            "args": [
                "job",
                "logs",
                "piro-build-job-test-branches.1"
            ]
        }
    ]
}
